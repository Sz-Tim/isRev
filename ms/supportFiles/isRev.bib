journal = {Ecology Letters},
keywords = {Biodiversity,Biological,Coral Reefs,Ecology,El Nino-Southern Oscillation,Folder - Gradients - General,Indonesia,Models,Multivariate Analysis,beta,methods,nestedness,theory,turnover},
mendeley-tags = {Folder - Gradients - General,beta,methods,nestedness,theory,turnover},
month = jan,
number = {1},
pages = {19--28},
pmid = {21070562},
